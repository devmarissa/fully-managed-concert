{
  "name": "fully-managed-concert",
  "tree": {
    "$className": "DataModel",
    
    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "init": {
        "$path": "assets/init.luau",
        "$className": "ModuleScript"
      },
      "Models": {
        "$path": "assets/models",
        "$className": "Folder"
      }
    },
    
    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },
    
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },
    
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}