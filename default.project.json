{
  "name": "fully-managed-concert",
  "tree": {
    "$className": "DataModel",
    
    "ReplicatedStorage": {
      "shared": {
        "$path": "src/shared"
      }
    },
    
    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },
    
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },
    
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}